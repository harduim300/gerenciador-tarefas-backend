{
  "version": 2,
  "builds": [
    {
      "src": "./dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth/login",
      "destination": "/dist/server.js"
    },
    {
      "src": "/auth/signup",
      "dest": "/dist/server.js"
    },
    {
      "src": "/auth/logout",
      "dest": "/dist/server.js"
    },
    {
      "src": "/tasks/(.*)",
      "dest": "./dist/server.js"
    },
    {
      "src": "/tasks/(.*)/share",
      "dest": "./dist/server.js"
    },
    {
      "src": "/usuario/(.*)",
      "dest": "./dist/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "./dist/server.js"
    }
  ]
}
  